@model TestAuth2Mvc.Models.MvcObject

@{
    ViewData["Title"] = "Edit";
}

<h1>Edit</h1>

<h4>MvcObject</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="MvcObjectId" />
            <div class="form-group">
                <label asp-for="MvcObjectName" class="control-label"></label>
                <input asp-for="MvcObjectName" class="form-control" />
                <span asp-validation-for="MvcObjectName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
